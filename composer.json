{
    "name": "johncarlo29/blade-cartzilla",
    "description": "A package to easily make use of Cartzilla in your Laravel Blade views.",
    "keywords": ["Blade", "Cartzilla", "Laravel"],
    "homepage": "https://github.com/JohnCarlo29/blade-cartzilla",
    "license": "MIT",
    "authors": [
        {
            "name": "John Carlo Advincula"
        }
    ],
    "require": {
        "php": "~8.2.0|~8.3.0",
        "blade-ui-kit/blade-icons": "^1.4",
        "illuminate/support": "^10.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "MerryMart\\Cartzilla\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "JohnCarlo29\\Cartzilla\\BladeCartzillaServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
